<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>com.simonferns.comfy</string>
        <key>ProgramArguments</key>
        <array>
          <string>/Users/<USERNAME>/ComfyUI/venv/bin/python</string>
          <string>/Users/<USERNAME>/ComfyUI/main.py</string>
          <string>--lowvram</string>
          <string>--listen</string>
        </array>
        <key>RunAtLoad</key>
        <true/>
        <key>StandardErrorPath</key>
        <string>/tmp/comfyui_public.err</string>
        <key>StandardOutPath</key>
        <string>/tmp/comfyui_public.out</string>
    </dict>
</plist>

